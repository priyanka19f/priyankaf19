pipeline {
    agent 
      node {
          label 'Dev1'
       }
    stages {
           stage ("cleaning stage") {
                  steps {
                        sh "maven clean"
                  }
           }
           stage ("testing stage")  {
                  steps {
                        sh "mvn test"
                  }
           }
           stage ("packaging stage") {
                  steps  {
                        sh "mvn package"
                  }
           }
     }
}     
                        
                        
    
